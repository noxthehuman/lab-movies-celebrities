<a href="/movies"> Go Back</a>


<div>
    <h2> {{movie.title}} </h2>
    <p> {{movie.genre}} </p>
    <p> {{movie.plot}} </p>

    <p> Cast </p>
    <ul> 
        {{#each movie.cast}}
        <li> {{this.name}} is a {{this.occupation}} and their catch phrase is {{this.catchPhrase}} </li>
        {{/each}}
    </ul>
</div>

<form action="/movies/{{movie._id}}/delete" method="post">
    <input type="submit" value="Delete" />
</form>
